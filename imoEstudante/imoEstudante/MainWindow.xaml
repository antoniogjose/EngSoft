<Window x:Class="ImoEstudante.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImoEstudante"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="799">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="260*"/>
            <ColumnDefinition Width="257*"/>
        </Grid.ColumnDefinitions>
        <Border BorderBrush="Black" BorderThickness="1" Grid.Column="1" HorizontalAlignment="Left" Height="240" Margin="0,10,0,0" VerticalAlignment="Top" Width="0"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="100" Margin="83,108,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
